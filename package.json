{
  "name": "elysia_betterauth_template",
  "version": "1.0.50",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "bun run --watch src/index.ts",
    "db:generate": "bun --env-file .env --bun drizzle-kit generate",
    "db:migrate": "bun --env-file .env --bun drizzle-kit migrate",
    "db:studio": "bun --env-file .env --bun drizzle-kit studio",
    "auth:generate" : "bunx --bun @better-auth/cli generate --output './src/database/schema/auth-schema.ts' --yes"
  },
  "dependencies": {
    "@elysiajs/openapi": "^1.4.13",
    "better-auth": "^1.4.10",
    "drizzle-orm": "^0.45.1",
    "drizzle-typebox": "^0.3.3",
    "drizzle-zod": "^0.8.3",
    "elysia": "latest",
    "pg": "^8.16.3",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "bun-types": "latest",
    "drizzle-kit": "^0.31.8"
  },
  "module": "src/index.js"
}
